{
  "name": "@stakewise/v3-sdk",
  "version": "0.1.0",
  "description": "StakeWise v3 SDK",
  "main": "dist/index.js",
  "typings": "types.d.ts",
  "scripts": {
    "test": "jest",
    "test-coverage": "jest --coverage",
    "graphql": "graphql-codegen && node ./scripts/generateGraphqlExports",
    "typechain": "typechain --target ethers-v6 --out-dir src/util/types 'src/util/abis/*.json'",
    "transpile": "babel ./src --out-dir dist --extensions .ts,.tsx --config-file ../../babel.config.js --delete-dir-on-start --no-comments --copy-files --no-copy-ignored",
    "generate": "npm run graphql",
    "todo_generate": "npm run graphql && npm run typechain",
    "build": "npm run generate && npm run transpile"
  },
  "license": "AGPL-3.0-only",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://v3.stakewise.io",
  "keywords": [
    "ethereum",
    "staking"
  ],
  "sideEffects": false,
  "devDependencies": {
    "@babel/cli": "7.22.10",
    "@babel/core": "7.22.10",
    "@faker-js/faker": "8.0.2",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/near-operation-file-preset": "2.5.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-operations": "4.0.1",
    "@openzeppelin/test-environment": "0.1.9",
    "hardhat": "2.17.1",
    "hardhat-jest-plugin": "0.0.6",
    "jest-fetch-mock": "3.0.3",
    "regenerator-runtime": "0.14.0",
    "typechain": "8.3.1",
    "typescript": "5.1.6"
  },
  "peerDependencies": {
    "ethers": "^6.7.1"
  }
}
